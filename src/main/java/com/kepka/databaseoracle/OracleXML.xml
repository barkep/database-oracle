<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.kepka.databaseoracle.model.CountriesEntity">
        <table name="COUNTRIES" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="countryId">
                <column name="COUNTRY_ID" length="2"/>
            </id>
            <basic name="countryName">
                <column name="COUNTRY_NAME" nullable="false" length="40"/>
            </basic>
            <many-to-one name="regionsByRegionId" optional="true"
                         target-entity="com.kepka.databaseoracle.model.RegionsEntity">
                <join-column name="REGION_ID" nullable="false" referenced-column-name="REGION_ID"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.DepartmentsEntity">
        <table name="DEPARTMENTS" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="departmentId">
                <column name="DEPARTMENT_ID" precision="4"/>
            </id>
            <basic name="departmentName">
                <column name="DEPARTMENT_NAME" length="30"/>
            </basic>
            <many-to-one name="employeesEntityByManagerId" optional="true"
                         target-entity="com.kepka.databaseoracle.model.EmployeesEntity">
                <join-column name="MANAGER_ID" nullable="false" referenced-column-name="EMPLOYEE_ID"/>
            </many-to-one>
            <many-to-one name="locationsByLocationId" optional="true"
                         target-entity="com.kepka.databaseoracle.model.LocationsEntity">
                <join-column name="LOCATION_ID" nullable="false" referenced-column-name="LOCATION_ID"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.DeptEntity">
        <table name="DEPT" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="7"/>
            </id>
            <basic name="name">
                <column name="NAME" nullable="false" length="25"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.EmpEntity">
        <table name="EMP" schema="ZIIBD21" catalog=""/>
        <attributes>
            <basic name="id">
                <column name="ID" nullable="false" precision="7"/>
            </basic>
            <basic name="lastName">
                <column name="LAST_NAME" nullable="false" length="25"/>
            </basic>
            <basic name="firstName">
                <column name="FIRST_NAME" nullable="false" length="25"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.EmployeesEntity">
        <table name="EMPLOYEES" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="employeeId">
                <column name="EMPLOYEE_ID" precision="6"/>
            </id>
            <basic name="firstName">
                <column name="FIRST_NAME" nullable="false" length="20"/>
            </basic>
            <basic name="lastName">
                <column name="LAST_NAME" length="25"/>
            </basic>
            <basic name="email">
                <column name="EMAIL" length="25"/>
            </basic>
            <basic name="phoneNumber">
                <column name="PHONE_NUMBER" nullable="false" length="20"/>
            </basic>
            <basic name="hireDate">
                <column name="HIRE_DATE" precision="-1"/>
            </basic>
            <basic name="salary">
                <column name="SALARY" nullable="false" precision="2"/>
            </basic>
            <basic name="commissionPct">
                <column name="COMMISSION_PCT" nullable="false" precision="2"/>
            </basic>
            <many-to-one name="jobsByJobId" optional="true" target-entity="com.kepka.databaseoracle.model.JobsEntity">
                <join-column name="JOB_ID" referenced-column-name="JOB_ID"/>
            </many-to-one>
            <many-to-one name="employeesEntityByManagerId" optional="true"
                         target-entity="com.kepka.databaseoracle.model.EmployeesEntity">
                <join-column name="MANAGER_ID" nullable="false" referenced-column-name="EMPLOYEE_ID"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.JobsEntity">
        <table name="JOBS" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="jobId">
                <column name="JOB_ID" length="10"/>
            </id>
            <basic name="jobTitle">
                <column name="JOB_TITLE" length="35"/>
            </basic>
            <basic name="minSalary">
                <column name="MIN_SALARY" nullable="false" precision="6"/>
            </basic>
            <basic name="maxSalary">
                <column name="MAX_SALARY" nullable="false" precision="6"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.JobGradesEntity">
        <table name="JOB_GRADES" schema="ZIIBD21" catalog=""/>
        <attributes>
            <basic name="gradeLevel">
                <column name="GRADE_LEVEL" nullable="false" length="3"/>
            </basic>
            <basic name="lowestSal">
                <column name="LOWEST_SAL" nullable="false" precision="2147483646"/>
            </basic>
            <basic name="highestSal">
                <column name="HIGHEST_SAL" nullable="false" precision="2147483646"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.JobHistoryEntity">
        <table name="JOB_HISTORY" schema="ZIIBD21" catalog=""/>
        <id-class class="com.kepka.databaseoracle.model.JobHistoryEntityPK"/>
        <attributes>
            <id name="employeeId">
                <column name="EMPLOYEE_ID" precision="6"/>
            </id>
            <id name="startDate">
                <column name="START_DATE" precision="-1"/>
            </id>
            <basic name="endDate">
                <column name="END_DATE" precision="-1"/>
            </basic>
            <many-to-one name="jobsByJobId" optional="true" target-entity="com.kepka.databaseoracle.model.JobsEntity">
                <join-column name="JOB_ID" referenced-column-name="JOB_ID"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.LocationsEntity">
        <table name="LOCATIONS" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="locationId">
                <column name="LOCATION_ID" precision="4"/>
            </id>
            <basic name="streetAddress">
                <column name="STREET_ADDRESS" nullable="false" length="40"/>
            </basic>
            <basic name="postalCode">
                <column name="POSTAL_CODE" nullable="false" length="12"/>
            </basic>
            <basic name="city">
                <column name="CITY" length="30"/>
            </basic>
            <basic name="stateProvince">
                <column name="STATE_PROVINCE" nullable="false" length="25"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.MyEmployeeEntity">
        <table name="MY_EMPLOYEE" schema="ZIIBD21" catalog=""/>
        <attributes>
            <basic name="id">
                <column name="ID" precision="4"/>
            </basic>
            <basic name="lastName">
                <column name="LAST_NAME" nullable="false" length="25"/>
            </basic>
            <basic name="firstName">
                <column name="FIRST_NAME" nullable="false" length="25"/>
            </basic>
            <basic name="userid">
                <column name="USERID" nullable="false" length="8"/>
            </basic>
            <basic name="salary">
                <column name="SALARY" nullable="false" precision="2"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.kepka.databaseoracle.model.RegionsEntity">
        <table name="REGIONS" schema="ZIIBD21" catalog=""/>
        <attributes>
            <id name="regionId">
                <column name="REGION_ID" precision="2147483646"/>
            </id>
            <basic name="regionName">
                <column name="REGION_NAME" nullable="false" length="25"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
